{
  "name": "ahrefs-continue",
  "icon": "media/a_ahrefs_logo.png",
  "version": "1.1.12",
  "repository": {
    "type": "git",
    "url": "https://github.com/ahrefs/ahrefs-continue"
  },
  "extensionKind": [
    "ui",
    "workspace"
  ],
  "bugs": {
    "url": "https://github.com/ahrefs/ahrefs-continue/issues",
    "email": "calvinn.ng@ahrefs.com"
  },
  "homepage": "https://ahrefs.com/",
  "qna": "https://github.com/ahrefs/ahrefs-continue/issues/new/choose",
  "displayName": "Ahrefs-Continue - In-House Coding Assistant",
  "pricing": "Free",
  "description": "Ahrefs' Internal Code Assistant :smiley:",
  "publisher": "Ahrefs",
  "engines": {
    "vscode": "^1.70.0",
    "node": ">=20.11.0"
  },
  "engine-strict": true,
  "galleryBanner": {
    "color": "#1E1E1E",
    "theme": "dark"
  },
  "categories": [
    "Programming Languages",
    "Education",
    "Machine Learning",
    "Snippets"
  ],
  "keywords": [
    "chatgpt",
    "github",
    "copilot",
    "llama",
    "codegpt",
    "ai"
  ],
  "activationEvents": [
    "onStartupFinished",
    "onView:ahrefs-continueGUIView"
  ],
  "main": "./out/extension.js",
  "browser": "./out/extension.js",
  "contributes": {
    "configuration": {
      "title": "Ahrefs-Continue",
      "properties": {
        "ahrefs-continue.telemetryEnabled": {
          "type": "boolean",
          "default": false,
          "markdownDescription": "Ahrefs-Continue collects anonymous usage data, cleaned of PII, to help us improve the product for our users. Read more  at [continue.dev › Telemetry](https://continue.dev/docs/telemetry)."
        },
        "ahrefs-continue.showInlineTip": {
          "type": "boolean",
          "default": true,
          "description": "Show inline suggestion to use the Ahrefs-Continue keyboard shortcuts (e.g. \"Cmd/Ctrl L to select code, Cmd/Ctrl I to edit\")."
        },
        "ahrefs-continue.enableTabAutocomplete": {
          "type": "boolean",
          "default": true,
          "markdownDescription": "Enable Ahrefs-Continue's tab autocomplete feature. Read our walkthrough to learn about configuration and how to share feedback: [continue.dev › Walkthrough: Tab Autocomplete](https://continue.dev/docs/walkthroughs/tab-autocomplete)"
        },
        "ahrefs-continue.remoteConfigServerUrl": {
          "type": "string",
          "default": "https://yep.tools/assets/ahrefs-continue-config.json",
          "markdownDescription": "If your team is set up to use shared configuration, enter the server URL here and your user token below to enable automatic syncing."
        },
        "ahrefs-continue.userToken": {
          "type": "string",
          "default": null,
          "markdownDescription": "If your team is set up to use shared configuration, enter your user token here and your server URL above to enable automatic syncing."
        },
        "ahrefs-continue.remoteConfigSyncPeriod": {
          "type": "number",
          "default": 60,
          "description": "The period of time in minutes between automatic syncs."
        },
        "ahrefs-continue.commandModel": {
          "type": "string",
          "default": null,
          "description": "Selected model to execute command"
        },
        "ahrefs-continue.completionModel": {
          "type": "string",
          "default": null,
          "description": "Selected model for code completion"
        }
      }
    },
    "commands": [
      {
        "command": "ahrefs-continue.selectCommandModel",
        "category": "Ahrefs-Continue",
        "title": "Select Command Execution Model (e.g. Ahrefs-Continue: Quick Edit)",
        "group": "Ahrefs-Continue"
      },
      {
        "command": "ahrefs-continue.selectCompletionModel",
        "category": "Ahrefs-Continue",
        "title": "Select Code Completion Model",
        "group": "Ahrefs-Continue"
      },
      {
        "command": "ahrefs-continue.acceptDiff",
        "category": "Ahrefs-Continue",
        "title": "Accept Diff",
        "group": "Ahrefs-Continue"
      },
      {
        "command": "ahrefs-continue.rejectDiff",
        "category": "Ahrefs-Continue",
        "title": "Reject Diff",
        "group": "Ahrefs-Continue",
        "icon": "$(stop)"
      },
      {
        "command": "ahrefs-continue.acceptVerticalDiffBlock",
        "category": "Ahrefs-Continue",
        "title": "Accept Vertical Diff Block",
        "group": "Ahrefs-Continue"
      },
      {
        "command": "ahrefs-continue.rejectVerticalDiffBlock",
        "category": "Ahrefs-Continue",
        "title": "Reject Vertical Diff Block",
        "group": "Ahrefs-Continue"
      },
      {
        "command": "ahrefs-continue.quickEdit",
        "category": "Ahrefs-Continue",
        "title": "Quick Edit",
        "group": "Ahrefs-Continue"
      },
      {
        "command": "ahrefs-continue.toggleAuxiliaryBar",
        "category": "Ahrefs-Continue",
        "title": "Toggle Right Sidebar",
        "group": "Ahrefs-Continue"
      },
      {
        "command": "ahrefs-continue.focusContinueInput",
        "category": "Ahrefs-Continue",
        "title": "Add Highlighted Code to Context",
        "group": "Ahrefs-Continue"
      },
      {
        "command": "ahrefs-continue.focusContinueInputWithoutClear",
        "category": "Ahrefs-Continue",
        "title": "Add Highlighted Code to Context",
        "group": "Ahrefs-Continue"
      },
      {
        "command": "ahrefs-continue.debugTerminal",
        "category": "Ahrefs-Continue",
        "title": "Debug Terminal",
        "group": "Ahrefs-Continue"
      },
      {
        "command": "ahrefs-continue.toggleFullScreen",
        "category": "Ahrefs-Continue",
        "title": "Toggle Full Screen",
        "icon": "$(fullscreen)",
        "group": "Ahrefs-Continue"
      },
      {
        "command": "ahrefs-continue.saveChatSession",
        "category": "Ahrefs-Continue",
        "title": "Save Chat Session",
        "group": "Ahrefs-Continue"
      },
      {
        "command": "ahrefs-continue.selectFilesAsContext",
        "category": "Ahrefs-Continue",
        "title": "Ahrefs-Continue: Select Files as Context",
        "group": "Ahrefs-Continue"
      },
      {
        "command": "ahrefs-continue.newSession",
        "category": "Ahrefs-Continue",
        "title": "New Session",
        "icon": "$(add)",
        "group": "Ahrefs-Continue"
      },
      {
        "command": "ahrefs-continue.viewHistory",
        "category": "Ahrefs-Continue",
        "title": "View History",
        "icon": "$(history)",
        "group": "Ahrefs-Continue"
      },
      {
        "command": "ahrefs-continue.toggleTabAutocompleteEnabled",
        "category": "Ahrefs-Continue",
        "title": "Toggle Autocomplete Enabled",
        "group": "Ahrefs-Continue"
      }
    ],
    "keybindings": [
      {
        "command": "ahrefs-continue.focusContinueInput",
        "mac": "cmd+l",
        "key": "ctrl+l"
      },
      {
        "command": "ahrefs-continue.focusContinueInputWithoutClear",
        "mac": "cmd+shift+l",
        "key": "ctrl+shift+l"
      },
      {
        "command": "ahrefs-continue.acceptDiff",
        "mac": "shift+cmd+enter",
        "key": "shift+ctrl+enter"
      },
      {
        "command": "ahrefs-continue.rejectDiff",
        "mac": "shift+cmd+backspace",
        "key": "shift+ctrl+backspace"
      },
      {
        "command": "ahrefs-continue.rejectDiff",
        "mac": "cmd+z",
        "key": "ctrl+z",
        "when": "ahrefs-continue.diffVisible"
      },
      {
        "command": "ahrefs-continue.acceptVerticalDiffBlock",
        "mac": "alt+cmd+y",
        "key": "alt+ctrl+y"
      },
      {
        "command": "ahrefs-continue.rejectVerticalDiffBlock",
        "mac": "alt+cmd+n",
        "key": "alt+ctrl+n"
      },
      {
        "command": "ahrefs-continue.quickEdit",
        "mac": "cmd+i",
        "key": "ctrl+i"
      },
      {
        "command": "ahrefs-continue.toggleAuxiliaryBar",
        "mac": "alt+cmd+l",
        "key": "alt+ctrl+l"
      },
      {
        "command": "ahrefs-continue.debugTerminal",
        "mac": "cmd+shift+r",
        "key": "ctrl+shift+r"
      },
      {
        "command": "ahrefs-continue.toggleFullScreen",
        "mac": "cmd+k cmd+m",
        "key": "ctrl+k ctrl+m",
        "when": "!terminalFocus"
      },
      {
        "command": "ahrefs-continue.toggleTabAutocompleteEnabled",
        "mac": "cmd+k cmd+a",
        "key": "ctrl+k ctrl+a"
      }
    ],
    "submenus": [
      {
        "id": "ahrefs-continue.ahrefs-continueSubMenu",
        "label": "Ahrefs-Continue"
      }
    ],
    "menus": {
      "commandPalette": [
        {
          "command": "ahrefs-continue.quickEdit"
        },
        {
          "command": "ahrefs-continue.toggleAuxiliaryBar"
        },
        {
          "command": "ahrefs-continue.focusContinueInput"
        },
        {
          "command": "ahrefs-continue.focusContinueInputWithoutClear"
        },
        {
          "command": "ahrefs-continue.debugTerminal"
        },
        {
          "command": "ahrefs-continue.toggleFullScreen"
        },
        {
          "command": "ahrefs-continue.newSession"
        }
      ],
      "editor/context": [
        {
          "submenu": "ahrefs-continue.ahrefs-continueSubMenu",
          "group": "0_aahrefs-continue"
        }
      ],
      "editor/title/run": [
        {
          "command": "ahrefs-continue.rejectDiff",
          "group": "Ahrefs-Continue",
          "when": "ahrefs-continue.streamingDiff"
        }
      ],
      "ahrefs-continue.ahrefs-continueSubMenu": [
        {
          "command": "ahrefs-continue.focusContinueInputWithoutClear",
          "group": "Ahrefs-Continue",
          "when": "editorHasSelection"
        }
      ],
      "explorer/context": [
        {
          "command": "ahrefs-continue.selectFilesAsContext",
          "group": "1_debug@1"
        }
      ],
      "view/title": [
        {
          "command": "ahrefs-continue.newSession",
          "group": "navigation@1",
          "when": "view == ahrefs-continue.ahrefs-continueGUIView"
        },
        {
          "command": "ahrefs-continue.toggleFullScreen",
          "group": "navigation@1",
          "when": "view == ahrefs-continue.ahrefs-continueGUIView"
        },
        {
          "command": "ahrefs-continue.viewHistory",
          "group": "navigation@1",
          "when": "view == ahrefs-continue.ahrefs-continueGUIView"
        }
      ],
      "editor/title": [
        {
          "command": "ahrefs-continue.toggleFullScreen",
          "group": "navigation@1",
          "when": "activeWebviewPanelId == ahrefs-continue.ahrefs-continueGUIView"
        }
      ]
    },
    "viewsContainers": {
      "activitybar": [
        {
          "id": "ahrefs-continue",
          "title": "Ahrefs-Continue",
          "icon": "media/a_ahrefs_square_bw_transparent_bg.png"
        }
      ]
    },
    "views": {
      "ahrefs-continue": [
        {
          "type": "webview",
          "id": "ahrefs-continue.ahrefs-continueGUIView",
          "name": "",
          "visibility": "visible"
        }
      ]
    },
    "walkthroughs": [
      {
        "id": "ahrefs-continue",
        "title": "Getting Started",
        "description": "Learn how to use Ahrefs-Continue",
        "steps": [
          {
            "id": "edit",
            "title": "Edit in natural language",
            "description": "Highlight a section of code and instruct Ahrefs-Continue to refactor it (e.g. `/edit rewrite this function to be async`)",
            "media": {
              "image": "media/edit.png",
              "altText": "Empty image"
            },
            "completionEvents": []
          },
          {
            "id": "explain",
            "title": "Get possible explanations",
            "description": "Ask Ahrefs-Continue about a part of your code to get another perspective (e.g. `where in the page should I be making this request to the backend?`)",
            "media": {
              "image": "media/explain.png",
              "altText": "Empty image"
            },
            "completionEvents": []
          },
          {
            "id": "generate",
            "title": "Generate files from scratch",
            "description": "Let Ahrefs-Continue build the scaffolding of Python scripts, React components, and more (e.g. `/edit here is a connector for postgres, now write one for kafka`)",
            "media": {
              "image": "media/generate.png",
              "altText": "Empty image"
            },
            "completionEvents": []
          }
        ]
      }
    ],
    "jsonValidation": [
      {
        "fileMatch": "config.json",
        "url": "./config_schema.json"
      },
      {
        "fileMatch": ".continuerc.json",
        "url": "./continue_rc_schema.json"
      }
    ]
  },
  "scripts": {
    "esbuild-base": "node esbuild.js",
    "vscode:prepublish": "npm run esbuild-base -- --minify",
    "esbuild": "npm run esbuild-base -- --sourcemap",
    "esbuild-watch": "npm run esbuild-base -- --sourcemap --watch",
    "tsc": "tsc -p ./",
    "tsc-watch": "tsc -watch -p ./",
    "rebuild": "electron-rebuild -v 19.1.8 node-pty",
    "lint": "eslint src --ext ts",
    "build-test": "tsc && node esbuild.test.mjs",
    "test": "npm run build-test && node ./out/test-runner/runTestOnVSCodeHost.js",
    "prepackage": "node scripts/prepackage.js",
    "package": "node scripts/package.js",
    "package:pre-release": "node scripts/package.js --pre-release",
    "build:rust": "cargo-cp-artifact -ac sync sync.node -- cargo build --manifest-path ../../sync/Cargo.toml --message-format=json-render-diagnostics",
    "build-debug:rust": "npm run build:rust --",
    "build-release:rust": "npm run build:rust -- --release"
  },
  "devDependencies": {
    "@nestjs/common": "^8.4.7",
    "@openapitools/openapi-generator-cli": "^2.5.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/follow-redirects": "^1.14.4",
    "@types/glob": "^8.0.0",
    "@types/fast-levenshtein": "^0.0.4",
    "@types/node": "16.x",
    "@types/react-dom": "^18.2.4",
    "@types/request": "^2.48.8",
    "@types/vscode": "1.70",
    "@types/ws": "^8.5.4",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "@vscode/vsce": "^2.22.0",
    "cargo-cp-artifact": "^0.1",
    "esbuild": "0.17.19",
    "eslint": "^8.28.0",
    "glob": "^8.0.3",
    "json-schema-to-typescript": "^12.0.0",
    "ovsx": "^0.8.3",
    "rimraf": "^5.0.5",
    "typescript": "^5.3.3",
    "vite": "^4.3.9",
    "vsce": "^2.15.0",
    "fast-levenshtein": "^3.0.0"
  },
  "dependencies": {
    "@electron/rebuild": "^3.2.10",
    "@reduxjs/toolkit": "^1.9.3",
    "@types/jest": "^29.5.11",
    "@types/node-fetch": "^2.6.11",
    "@types/fast-levenshtein": "^0.0.4",
    "@vscode/ripgrep": "^1.15.9",
    "@vscode/test-electron": "^2.3.9",
    "axios": "^1.2.5",
    "core": "file:../../core",
    "cors": "^2.8.5",
    "dbinfoz": "^0.1.4",
    "downshift": "^7.6.0",
    "express": "^4.18.2",
    "fkill": "^8.1.0",
    "follow-redirects": "^1.15.4",
    "handlebars": "^4.7.8",
    "highlight.js": "^11.7.0",
    "highlightable": "^1.3.0-beta.0",
    "http-proxy": "^1.18.1",
    "http-proxy-agent": "^7.0.0",
    "http-proxy-middleware": "^2.0.6",
    "https-proxy-agent": "^7.0.2",
    "ignore": "^5.3.0",
    "jest": "^29.7.0",
    "jsdom": "^24.0.0",
    "mocha": "^10.3.0",
    "monaco-editor": "^0.45.0",
    "monaco-vscode-textmate-theme-converter": "^0.1.7",
    "ncp": "^2.0.0",
    "node-fetch": "^3.3.2",
    "node-machine-id": "^1.1.12",
    "posthog-node": "^3.1.2",
    "react-markdown": "^8.0.7",
    "react-redux": "^8.0.5",
    "request": "^2.88.2",
    "socket.io-client": "^4.7.2",
    "strip-ansi": "^7.1.0",
    "tailwindcss": "^3.3.2",
    "undici": "^6.2.0",
    "uuidv4": "^6.2.13",
    "vectordb": "0.4.12",
    "vscode-languageclient": "^8.0.2",
    "ws": "^8.17.1",
    "yarn": "^1.22.21",
    "fast-levenshtein": "^3.0.0"
  }
}